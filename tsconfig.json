{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "alwaysStrict": true,
    "esModuleInterop": true,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "moduleResolution": "node",
    "lib": ["ES2019"],
    "module": "commonjs",
    "target": "ES2019",
    "outDir": "dist",
    "resolveJsonModule": true
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts"]
}
